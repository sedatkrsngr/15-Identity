@model IQueryable<AppUser>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<table class="table">
    
    <thead>
        <tr>
            <th>User Id</th>
            <th>User Name</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        @if (Model.Count() == 0)
        {
            <tr>
                <td colspan="3">Hiç bir üye yoktur</td>
            </tr>
        }
        else
        {
            foreach (var item in Model)
            {
             <tr>@item.Id</tr>
             <tr>@item.UserName</tr>
             <tr>@item.Email</tr>
            }

        }
    </tbody>

</table>

